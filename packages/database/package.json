    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "6.18.0",
    "dotenv": "17.2.3"
  },
  "devDependencies": {
    "prisma": "6.18.0",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  }
